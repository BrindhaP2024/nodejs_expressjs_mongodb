<!-- views/index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>User Profile</title>
</head>
<body>
  <h1>Welcome, <%= name %>!</h1>
  <p>Age: <%= age %></p>
  <h2>Hobbies:</h2>
  <ul>
    <% hobbies.forEach(function(hobby) { %>
      <li><%= hobby %></li>
    <% }); %>
  </ul>
</body>
</html>
